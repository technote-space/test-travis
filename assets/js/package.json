{
  "name": "test-travis",
  "version": "0.0.1",
  "description": "Test for Travis CI",
  "scripts": {
    "start": "npm run build",
    "test": "npm run lint && npm run jest",
    "test:update": "npm run lint && npm run jest:update",
    "lint": "eslint src/**/**/*.js && eslint __tests__/**/**/*.js",
    "jest": "jest --coverage",
    "jest:update": "jest --coverage --updateSnapshot",
    "develop": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "coveralls": "cat ./coverage/lcov.info | $(npm bin)/coveralls"
  },
  "author": "Technote <technote.space@gmail.com> (https://technote.space)",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.4",
    "css-loader": "^3.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "webpack": "^4.34.0",
    "webpack-cli": "~3.3.4"
  }
}
